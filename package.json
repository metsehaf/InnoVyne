{
  "name": "innovyne",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:client": "vite client --config client/vite.config.ts",
    "build:client": "vite build client --config client/vite.config.ts",
    "preview:client": "vite preview client --config client/vite.config.ts",
    "dev:server": "tsx watch backend/src/index.ts",
    "build:server": "tsc -p tsconfig.json --outDir dist",
    "start:server": "node dist/index.js",
    "start": "concurrently \"npm run dev:client\" \"npm run dev:server\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "tsx": "^3.12.5",
    "typescript": "^5.5.0",
    "vite": "^5.0.0"
  }
}
