{
  "name": "innovyne",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:client": "vite client --config client/vite.config.ts",
    "build:client": "vite build client --config client/vite.config.ts",
    "preview:client": "vite preview client --config client/vite.config.ts",
    "dev:server": "tsx watch backend/src/index.ts",
    "build:server": "tsc -p tsconfig.json --outDir dist",
    "start:server": "node dist/index.js",
    "start": "concurrently \"npm run dev:client\" \"npm run dev:server\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "joi": "^18.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.1.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^3.12.5",
    "typescript": "^5.5.0",
    "vite": "^5.0.0"
  }
}
